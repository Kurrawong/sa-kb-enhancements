        <https://gcmd.earthdata.nasa.gov/kms/concept/585182e9-6e5b-4ad6-96fe-065ffd31f7e8> ;
    skos:prefLabel "ENVIRONMENTAL VULNERABILITY INDEX (EVI)"@en ;
    gcmd:reference
